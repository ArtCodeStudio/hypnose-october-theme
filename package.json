{
  "name": "hypnose-october-theme",
  "version": "1.7.0",
  "description": "Hypnose OctoberCMS Theme",
  "main": "assets/ts/main.ts",
  "typings": "assets/ts/types",
  "scripts": {
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts' && tsc --noEmit",
    "test": "npm run lint",
    "build": "concurrently \"npm:build:prod:*\"",
    "build:prod:ts": "npm run lint && npm run webpack",
    "build:dev": "concurrently \"npm:build:dev:*\"",
    "build:dev:ts": "npm run lint && npm run webpack:dev",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:ts": "npm run webpack:dev -- --watch",
    "packages:link": "npm link @ribajs/core @ribajs/bs4 @ribajs/router @ribajs/iconset @ribajs/extras @ribajs/jquery"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/doc"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "OctoberCMS",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "Hypnose",
    "hypnosis"
  ],
  "author": "Art+Code Studio <hi@artandcode.studio>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ArtCodeStudio/hypnose-october-theme/blob/master/README.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/ArtCodeStudio/hypnose-october-theme/issues"
  },
  "url": "https://github.com/ArtCodeStudio/hypnose-october-theme",
  "homepage": "https://hypnose.jumplink.eu/",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-export-default-from": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime-corejs3": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babel-plugin-array-includes": "^2.0.3",
    "concurrently": "^5.0.2",
    "core-js": "^3.6.2",
    "html-loader": "^0.5.5",
    "inspectpack": "^4.3.0",
    "pug-loader": "^2.4.0",
    "terser-webpack-plugin": "^2.3.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@ribajs/bs4": "^1.8.0",
    "@ribajs/core": "^1.8.0",
    "@ribajs/iconset": "^1.8.0",
    "@ribajs/router": "^1.8.0",
    "@ribajs/extras": "^1.8.0",
    "@ribajs/jquery": "^1.8.0",
    "bootstrap": "^4.4.1"
  }
}
